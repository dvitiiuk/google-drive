{
  "metadata": {
    "spec-version": "1.5"
  },
  "display-name": "Google Drive source",
  "configuration-groups": [
    {
      "label": "Basic",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        },
        {
          "widget-type": "textbox",
          "label": "App Id",
          "name": "appId"
        },
        {
          "widget-type": "textbox",
          "label": "Access Token",
          "name": "accessToken"
        },
        {
          "widget-type": "textbox",
          "label": "Directory identifier",
          "name": "directoryIdentifier"
        },
        {
          "widget-type": "textbox",
          "label": "Filter",
          "name": "filter"
        },
        {
          "widget-type": "textbox",
          "label": "Modification date range",
          "name": "modificationDateRange"
        },
        {
          "name": "metainfos",
          "widget-type": "multi-select",
          "label": "File properties",
          "widget-attributes": {
            "delimiter": ",",
            "options": [
              {
                "id": "id",
                "label": "ID"
              },
              {
                "id": "name",
                "label": "Name"
              },
              {
                "id": "mimeType",
                "label": "MIME Type"
              },
              {
                "id": "description",
                "label": "Description"
              },
              {
                "id": "starred",
                "label": "Starred"
              },
              {
                "id": "trashed",
                "label": "Trashed"
              },
              {
                "id": "explicitlyTrashed",
                "label": "Explicitly trashed"
              },
              {
                "id": "parents",
                "label": "Parents"
              },
              {
                "id": "properties",
                "label": "Properties"
              },
              {
                "id": "imageMediaMetadata.width",
                "label": "Width"
              },
              {
                "id": "imageMediaMetadata.height",
                "label": "Height"
              },
              {
                "id": "imageMediaMetadata.rotation",
                "label": "Rotation"
              },
              {
                "id": "imageMediaMetadata.location.latitude",
                "label": "Latitude"
              },
              {
                "id": "imageMediaMetadata.location.longitude",
                "label": "Longitude"
              },
              {
                "id": "imageMediaMetadata.location.altitude",
                "label": "Altitude"
              },
              {
                "id": "imageMediaMetadata.time",
                "label": "Time"
              },
              {
                "id": "imageMediaMetadata.cameraMake",
                "label": "Camera"
              },
              {
                "id": "imageMediaMetadata.cameraModel",
                "label": "Camera model"
              },
              {
                "id": "imageMediaMetadata.exposureTime",
                "label": "Exposure"
              },
              {
                "id": "imageMediaMetadata.aperture",
                "label": "Aperture"
              },
              {
                "id": "imageMediaMetadata.flashUsed",
                "label": "Used flash"
              },
              {
                "id": "imageMediaMetadata.focalLength",
                "label": "Focal length"
              },
              {
                "id": "imageMediaMetadata.isoSpeed",
                "label": "ISO"
              },
              {
                "id": "imageMediaMetadata.meteringMode",
                "label": "Metering mode"
              },
              {
                "id": "imageMediaMetadata.sensor",
                "label": "Sensor"
              },
              {
                "id": "imageMediaMetadata.exposureMode",
                "label": "Exposure mode"
              },
              {
                "id": "imageMediaMetadata.colorSpace",
                "label": "Color space"
              },
              {
                "id": "imageMediaMetadata.whiteBalance",
                "label": "Color space"
              },
              {
                "id": "imageMediaMetadata.exposureBias",
                "label": "Color space"
              }
            ]
          }
        },
        {
          "name": "fileTypesToPull",
          "widget-type": "multi-select",
          "label": "File types to pull",
          "widget-attributes": {
            "delimiter": ",",
            "default": [
              "binary"
            ],
            "options": [
              {
                "id": "binary",
                "label": "Binary"
              },
              {
                "id": "documents",
                "label": "Google Documents"
              },
              {
                "id": "spreadsheets",
                "label": "Google Spreadsheets"
              },
              {
                "id": "drawings",
                "label": "Google Drawings"
              },
              {
                "id": "presentations",
                "label": "Google Presentations"
              },
              {
                "id": "appsScripts",
                "label": "Google Apps Scripts"
              }
            ]
          }
        }
      ]
    },
    {
      "label": "Exporting",
      "properties": [
        {
          "widget-type": "select",
          "label": "Google Documents export format",
          "name": "docsExportingFormat",
          "widget-attributes": {
            "default": "text/plain",
            "values": [
              "text/html",
              "application/zip",
              "text/plain",
              "application/rtf",
              "application/vnd.oasis.opendocument.text",
              "application/pdf",
              "application/vnd.openxmlformats-officedocument.wordprocessingml.document",
              "application/epub+zip"
            ]
          }
        },
        {
          "widget-type": "select",
          "name": "sheetsExportingFormat",
          "label": "Google Spreadsheets export format",
          "widget-attributes": {
            "default": "text/csv",
            "values": [
              "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet",
              "application/x-vnd.oasis.opendocument.spreadsheet",
              "application/pdf",
              "text/csv",
              "text/tab-separated-values",
              "application/zip"
            ]
          }
        },
        {
          "widget-type": "select",
          "name": "drawingsExportingFormat",
          "label": "Google Drawings export format",
          "widget-attributes": {
            "default": "image/svg+xml",
            "values": [
              "image/jpeg",
              "image/png",
              "image/svg+xml",
              "application/pdf"
            ]
          }
        },
        {
          "widget-type": "select",
          "name": "presentationsExportingFormat",
          "label": "Google Presentations export format",
          "widget-attributes": {
            "default": "text/plain",
            "values": [
              "application/vnd.openxmlformats-officedocument.presentationml.presentation",
              "application/vnd.oasis.opendocument.presentation",
              "application/pdf",
              "text/plain"
            ]
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "widget-type": "non-editable-schema-editor",
      "schema": {
      }
    }
  ]
}
